<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%-d{yyyy-MM-dd HH:mm:ss}  [ %t:%r ] - [ %p ] %l %m%n" />
        </Console>
    </Appenders>
    <Loggers>
        <root level="info">
            <appender-ref ref="Console"/>
        </root>
    </Loggers>
</Configuration>